<?xml version='1.0' encoding='utf-8'?>
<widget id="org.luke.awesomeness" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>org.luke.awesomeness</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="callback-dev@incubator.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <icon height="512" src="res/icon/cordova_512.png" width="512" />
    <icon gap:platform="android" height="96" src="res/icon/cordova_android_96.png" width="96" />
    <icon gap:platform="blackberry" height="80" src="res/icon/cordova_bb_80.png" width="80" />
    <icon gap:platform="ios" height="144" src="res/icon/cordova_ios_144.png" width="144" />
    <gap:splash gap:platform="android" height="480" src="res/screen/android_hdpi_landscape.png" width="800" />
    <gap:splash gap:platform="android" height="800" src="res/screen/android_hdpi_portrait.png" width="480" />
    <gap:splash gap:platform="android" height="200" src="res/screen/android_ldpi_landscape.png" width="320" />
    <gap:splash gap:platform="android" height="320" src="res/screen/android_ldpi_portrait.png" width="200" />
    <gap:splash gap:platform="android" height="320" src="res/screen/android_mdpi_landscape.png" width="480" />
    <gap:splash gap:platform="android" height="480" src="res/screen/android_mdpi_portrait.png" width="320" />
    <gap:splash gap:platform="android" height="720" src="res/screen/android_xhdpi_landscape.png" width="1280" />
    <gap:splash gap:platform="android" height="1280" src="res/screen/android_xhdpi_portrait.png" width="720" />
    <gap:splash gap:platform="blackberry" height="300" src="res/screen/blackberry_transparent_300.png" width="300" />
    <gap:splash gap:platform="blackberry" height="200" src="res/screen/blackberry_transparent_400.png" width="200" />
    <gap:splash gap:platform="ios" height="748" src="res/screen/ipad_landscape.png" width="1024" />
    <gap:splash gap:platform="ios" height="1004" src="res/screen/ipad_portrait.png" width="768" />
    <gap:splash gap:platform="ios" height="1496" src="res/screen/ipad_retina_landscape.png" width="2048" />
    <gap:splash gap:platform="ios" height="2008" src="res/screen/ipad_retina_portrait.png" width="1536" />
    <gap:splash gap:platform="ios" height="320" src="res/screen/iphone_landscape.png" width="480" />
    <gap:splash gap:platform="ios" height="480" src="res/screen/iphone_portrait.png" width="320" />
    <gap:splash gap:platform="ios" height="640" src="res/screen/iphone_retina_landscape.png" width="960" />
    <gap:splash gap:platform="ios" height="960" src="res/screen/iphone_retina_portrait.png" width="640" />
    <gap:splash gap:platform="winphone" height="800" src="res/screen/windows_phone_portrait.jpg" width="480" />
    <feature name="http://api.phonegap.com/1.0/device" />
    <preference name="phonegap-version" value="1.9.0" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <access origin="*" />
            <plugin name="App" value="org.apache.cordova.App" />
        <plugin name="Geolocation" value="org.apache.cordova.GeoBroker" />
        <plugin name="Device" value="org.apache.cordova.Device" />
        <plugin name="Accelerometer" value="org.apache.cordova.AccelListener" />
        <plugin name="Compass" value="org.apache.cordova.CompassListener" />
        <plugin name="Media" value="org.apache.cordova.AudioHandler" />
        <plugin name="Camera" value="org.apache.cordova.CameraLauncher" />
        <plugin name="Contacts" value="org.apache.cordova.ContactManager" />
        <plugin name="File" value="org.apache.cordova.FileUtils" />
        <plugin name="NetworkStatus" value="org.apache.cordova.NetworkManager" />
        <plugin name="Notification" value="org.apache.cordova.Notification" />
        <plugin name="Storage" value="org.apache.cordova.Storage" />
        <plugin name="FileTransfer" value="org.apache.cordova.FileTransfer" />
        <plugin name="Capture" value="org.apache.cordova.Capture" />
        <plugin name="Battery" value="org.apache.cordova.BatteryListener" />
        <plugin name="SplashScreen" value="org.apache.cordova.SplashScreen" />
        <plugin name="Echo" value="org.apache.cordova.Echo" />
        <plugin name="Globalization" value="org.apache.cordova.Globalization" />
        <plugin name="InAppBrowser" value="org.apache.cordova.InAppBrowser" />
        <plugin name="PushPlugin" value="com.plugin.GCM.PushPlugin" />
</widget>
